{
  "extends": [
    "eslint-config-poetez/typescript-react",
    "react-app",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "semi": "off",
    "sort-keys": "off",
    "global-require": "off",
    "spaced-comment": "off",
    "capitalized-comments": "off",
    "padding-line-between-statements": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}